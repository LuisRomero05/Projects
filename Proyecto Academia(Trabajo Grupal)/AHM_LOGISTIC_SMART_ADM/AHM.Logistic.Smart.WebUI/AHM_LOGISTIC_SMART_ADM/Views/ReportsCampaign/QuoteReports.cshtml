<!DOCTYPE html>
<html>
<head>
</head>
<body>

    <div class="card">
        <div class="card-header">
            <div class="row">
                <div class="col-md-6">
                    <h3>Reportes de cotizaciones</h3>
                </div>
            </div>
        </div>
        <div>
            <div class="card-body" style="padding:1%">
                <div class="form-row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <div class="form-group">
                                <label class="col-form-label required">Cliente a seleccionar</label>
                                <select id="SelectCustomers" class="form-control select2"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <div class="form-group">
                                <label class="col-form-label required">Fecha de inicio</label>
                                <input class="form-control" type="date" id="fechaInicio" onkeydown="return false" required />
                            </div>
                            <span class="text-danger" id="validarExistencia"></span>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label class="col-form-label required">Fecha final</label>
                        <div class="input-group">
                            <input class="form-control" type="date" value="" id="fechaFinal" onkeydown="return false" />
                        </div>
                    </div>
                    <div class="col-md-3" style="padding-top: 35px">
                        <button class="btn btn-primary" tabindex="0" onclick="ReporteDinamico()" type="button" title="Mostrar reporte"><i class="fa fa-file-text"></i> Mostrar</button>
                        <button class="btn btn-primary" tabindex="0" onclick="LimpiarFechas()" type="button"><i class="fa fa-file-text"></i>Limpiar</button>
                    </div>
                </div>
                <div class="card-box task-detail text-center" id="divFrame">
                    <div class="sk-fading-circle" id="divCarga" style="display:none">
                        <div class="sk-circle1 sk-circle"></div>
                        <div class="sk-circle2 sk-circle"></div>
                        <div class="sk-circle3 sk-circle"></div>
                        <div class="sk-circle4 sk-circle"></div>
                        <div class="sk-circle5 sk-circle"></div>
                        <div class="sk-circle6 sk-circle"></div>
                        <div class="sk-circle7 sk-circle"></div>
                        <div class="sk-circle8 sk-circle"></div>
                        <div class="sk-circle9 sk-circle"></div>
                        <div class="sk-circle10 sk-circle"></div>
                        <div class="sk-circle11 sk-circle"></div>
                        <div class="sk-circle12 sk-circle"></div>
                    </div>
                    <iframe id="frmReporte" style="width:100%; height:130vh; width:130vh; padding-top:40px" frameborder="0" scrolling="no"></iframe>
                </div>
            </div>
        </div>
    </div>

    <br />

</body>
</html>
<script src="https://code.jquery.com/jquery-3.6.0.js"></script>
<script src="~/js/app/Reports/QuoteReports.js"></script>
