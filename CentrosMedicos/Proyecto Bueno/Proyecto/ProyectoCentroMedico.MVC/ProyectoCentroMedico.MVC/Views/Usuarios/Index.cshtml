@model IEnumerable<UsuariosViewModel>
@{
    ViewData["Title"] = "Usuarios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />  <p><br /></p>
<div class="container col-lg-10">
    <div class="card">
        <div class="card-header col-lg-12">
            <br />
            <h4>Listado de Usuarios</h4><br />
        </div>
        <div class="card-body">
            <p>
                <br />
                <a class="btn btn-sm btn-outline-success" asp-area="" asp-controller="Usuarios" asp-action="Create">Insertar</a>

            </p>

            <table class="table table-hover table-light table-striped" id="example1">
                <thead class="thead-dark">
                    <tr>
                        <th>
                            Rol
                        </th>
                        <th>
                            Nombre
                        </th>
                        <th>
                            Apellido
                        </th>

                        <th>
                            Email
                        </th>
                        <th>
                            Celular
                        </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.rol_ID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.usu_Nombre)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.usu_Apellido)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.usu_Email)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.usu_NumeroCelular)
                            </td>


                        </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>

</div>

